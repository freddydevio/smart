<div class="grid--item news--widget size--{{ item.size }} module--id-{{ item.moduleId }}">
    <div class="row-content">
        <i class="zmdi zmdi-rss"></i>
        <span class="widget-title">Nachrichten</span>
    </div>
    {% for newsItem in item.data %}
        <div class="row">
            <span class="title">{{ newsItem.title }}</span>
        </div>
    {% endfor %}
</div>